<?xml version="1.0" encoding="UTF-8"?>
<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <map>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:location_manager/Resources/Private/Language/locallang.xlf:plugin.locations.sheets.map
                    </sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.map.mapTypeId>
                        <TCEforms>
                            <label>
                                LLL:EXT:location_manager/Resources/Private/Language/locallang.xlf:plugin.locations.settings.map.mapTypeId
                            </label>
                            <config>
                                <type>select</type>
                                <items>
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">
                                            LLL:EXT:location_manager/Resources/Private/Language/locallang.xlf:plugin.locations.settings.map.mapTypeId.road
                                        </numIndex>
                                        <!-- google.maps.MapTypeId.ROADMAP -->
                                        <numIndex index="1">roadmap</numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">
                                            LLL:EXT:location_manager/Resources/Private/Language/locallang.xlf:plugin.locations.settings.map.mapTypeId.satellite
                                        </numIndex>
                                        <!-- google.maps.MapTypeId.SATELLITE -->
                                        <numIndex index="1">satellite</numIndex>
                                    </numIndex>
                                    <numIndex index="2" type="array">
                                        <numIndex index="0">
                                            LLL:EXT:location_manager/Resources/Private/Language/locallang.xlf:plugin.locations.settings.map.mapTypeId.terrain
                                        </numIndex>
                                        <!-- google.maps.MapTypeId.TERRAIN -->
                                        <numIndex index="1">terrain</numIndex>
                                    </numIndex>
                                    <numIndex index="3" type="array">
                                        <numIndex index="0">
                                            LLL:EXT:location_manager/Resources/Private/Language/locallang.xlf:plugin.locations.settings.map.mapTypeId.hybrid
                                        </numIndex>
                                        <!-- google.maps.MapTypeId.HYBRID -->
                                        <numIndex index="1">hybrid</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </settings.map.mapTypeId>

                    <settings.map.center.latitude>
                        <TCEforms>
                            <label>
                                LLL:EXT:location_manager/Resources/Private/Language/locallang.xlf:plugin.locations.settings.map.center.latitude
                            </label>
                            <config>
                                <type>input</type>
                            </config>
                        </TCEforms>
                    </settings.map.center.latitude>

                    <settings.map.center.longitude>
                        <TCEforms>
                            <label>
                                LLL:EXT:location_manager/Resources/Private/Language/locallang.xlf:plugin.locations.settings.map.center.longitude
                            </label>
                            <config>
                                <type>input</type>
                            </config>
                        </TCEforms>
                    </settings.map.center.longitude>

                    <settings.map.zoom>
                        <TCEforms>
                            <label>
                                LLL:EXT:location_manager/Resources/Private/Language/locallang.xlf:plugin.locations.settings.map.zoom
                            </label>
                            <config>
                                <type>select</type>
                                <default>7</default>
                                <items>
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">0</numIndex>
                                        <numIndex index="1">0</numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">1</numIndex>
                                        <numIndex index="1">1</numIndex>
                                    </numIndex>
                                    <numIndex index="2" type="array">
                                        <numIndex index="0">2</numIndex>
                                        <numIndex index="1">2</numIndex>
                                    </numIndex>
                                    <numIndex index="3" type="array">
                                        <numIndex index="0">3</numIndex>
                                        <numIndex index="1">3</numIndex>
                                    </numIndex>
                                    <numIndex index="4" type="array">
                                        <numIndex index="0">4</numIndex>
                                        <numIndex index="1">4</numIndex>
                                    </numIndex>
                                    <numIndex index="5" type="array">
                                        <numIndex index="0">5</numIndex>
                                        <numIndex index="1">5</numIndex>
                                    </numIndex>
                                    <numIndex index="6" type="array">
                                        <numIndex index="0">6</numIndex>
                                        <numIndex index="1">6</numIndex>
                                    </numIndex>
                                    <numIndex index="7" type="array">
                                        <numIndex index="0">7</numIndex>
                                        <numIndex index="1">7</numIndex>
                                    </numIndex>
                                    <numIndex index="8" type="array">
                                        <numIndex index="0">8</numIndex>
                                        <numIndex index="1">8</numIndex>
                                    </numIndex>
                                    <numIndex index="9" type="array">
                                        <numIndex index="0">9</numIndex>
                                        <numIndex index="1">9</numIndex>
                                    </numIndex>
                                    <numIndex index="10" type="array">
                                        <numIndex index="0">10</numIndex>
                                        <numIndex index="1">10</numIndex>
                                    </numIndex>
                                    <numIndex index="11" type="array">
                                        <numIndex index="0">11</numIndex>
                                        <numIndex index="1">11</numIndex>
                                    </numIndex>
                                    <numIndex index="12" type="array">
                                        <numIndex index="0">12</numIndex>
                                        <numIndex index="1">12</numIndex>
                                    </numIndex>
                                    <numIndex index="13" type="array">
                                        <numIndex index="0">13</numIndex>
                                        <numIndex index="1">13</numIndex>
                                    </numIndex>
                                    <numIndex index="14" type="array">
                                        <numIndex index="0">14</numIndex>
                                        <numIndex index="1">14</numIndex>
                                    </numIndex>
                                    <numIndex index="15" type="array">
                                        <numIndex index="0">15</numIndex>
                                        <numIndex index="1">15</numIndex>
                                    </numIndex>
                                    <numIndex index="16" type="array">
                                        <numIndex index="0">16</numIndex>
                                        <numIndex index="1">16</numIndex>
                                    </numIndex>
                                    <numIndex index="17" type="array">
                                        <numIndex index="0">17</numIndex>
                                        <numIndex index="1">17</numIndex>
                                    </numIndex>
                                    <numIndex index="18" type="array">
                                        <numIndex index="0">18</numIndex>
                                        <numIndex index="1">18</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </settings.map.zoom>

                    <settings.map.zoomControl>
                        <TCEforms>
                            <label>
                                LLL:EXT:location_manager/Resources/Private/Language/locallang.xlf:plugin.locations.settings.map.zoomControl
                            </label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </settings.map.zoomControl>

                    <settings.map.enableLocking>
                        <TCEforms>
                            <label>
                                LLL:EXT:location_manager/Resources/Private/Language/locallang.xlf:plugin.locations.settings.map.enableLocking
                            </label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </settings.map.enableLocking>

                    <settings.map.hideOnMobile>
                        <TCEforms>
                            <label>
                                LLL:EXT:location_manager/Resources/Private/Language/locallang.xlf:plugin.locations.settings.map.hideOnMobile
                            </label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </settings.map.hideOnMobile>

                </el>
            </ROOT>
        </map>

        <content>
            <ROOT>
                <TCEforms>
                    <sheetTitle>
                        LLL:EXT:location_manager/Resources/Private/Language/locallang.xlf:plugin.locations.sheets.content
                    </sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.content.fixed>
                        <TCEforms>
                            <label>
                                LLL:EXT:location_manager/Resources/Private/Language/locallang.xlf:plugin.locations.settings.content.fixed
                            </label>
                            <config>
                                <type>select</type>
                                <foreign_table>tx_locationmanager_domain_model_location</foreign_table>
                                <minitems>0</minitems>
                                <maxitems>99</maxitems>
                                <multiple>1</multiple>
                                <renderType>selectMultipleSideBySide</renderType>
                            </config>
                        </TCEforms>
                    </settings.content.fixed>
                </el>
            </ROOT>
        </content>

        <search>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:location_manager/Resources/Private/Language/locallang.xlf:plugin.locations.sheets.search
                    </sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.search.enable>
                        <TCEforms>
                            <label>
                                LLL:EXT:location_manager/Resources/Private/Language/locallang.xlf:plugin.locations.settings.search.enable
                            </label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </settings.search.enable>

                    <settings.search.expand>
                        <TCEforms>
                            <label>
                                LLL:EXT:location_manager/Resources/Private/Language/locallang.xlf:plugin.locations.settings.search.expand
                            </label>
                            <config>
                                <type>input</type>
                                <eval>int</eval>
                                <placeholder>km</placeholder>
                            </config>
                        </TCEforms>
                    </settings.search.expand>

                    <settings.filter.enable>
                        <TCEforms>
                            <label>
                                LLL:EXT:location_manager/Resources/Private/Language/locallang.xlf:plugin.locations.settings.filter.enable
                            </label>
                            <onChange>reload</onChange>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </settings.filter.enable>

                    <settings.filter.preselect>
                        <TCEforms>
                            <label>
                                LLL:EXT:location_manager/Resources/Private/Language/locallang.xlf:plugin.locations.settings.filter.preselect
                            </label>
                            <!-- only show if settings.filter.enable is true -->
                            <displayCond>FIELD:settings.filter.enable:=:1</displayCond>
                            <config>
                                <type>select</type>
                                <foreign_table>sys_category</foreign_table>
                                <minitems>0</minitems>
                                <maxitems>99</maxitems>
                                <multiple>1</multiple>
                                <renderType>selectTree</renderType>
                                <treeConfig>
                                    <parentField>parent</parentField>
                                </treeConfig>
                            </config>
                        </TCEforms>
                    </settings.filter.preselect>

                    <settings.filter.combine>
                        <label>
                            LLL:EXT:location_manager/Resources/Private/Language/locallang.xlf:plugin.locations.settings.filter.combine
                        </label>
                        <displayCond>FIELD:settings.filter.enable:=:1</displayCond>
                        <config>
                            <type>select</type>
                            <items>
                                <numIndex index="0">
                                    <numIndex index="0">none</numIndex>
                                    <numIndex index="1">none</numIndex>
                                </numIndex>
                                <numIndex index="1">
                                    <numIndex index="0">AND</numIndex>
                                    <numIndex index="1">AND</numIndex>
                                </numIndex>
                                <numIndex index="2">
                                    <numIndex index="0">OR</numIndex>
                                    <numIndex index="1">OR</numIndex>
                                </numIndex>
                            </items>
                            <default>none</default>
                        </config>
                    </settings.filter.combine>
                </el>
            </ROOT>
        </search>


        <advanced>
            <ROOT>
                <TCEforms>
                    <sheetTitle>
                        LLL:EXT:location_manager/Resources/Private/Language/locallang.xlf:plugin.locations.sheets.advanced
                    </sheetTitle>
                </TCEforms>
                <type>array</type>

                <el>
                    <settings.map.style>
                        <TCEforms>
                            <label>
                                LLL:EXT:location_manager/Resources/Private/Language/locallang.xlf:plugin.locations.settings.advanced.style
                            </label>
                            <config>
                                <type>text</type>
                            </config>
                        </TCEforms>
                    </settings.map.style>

                    <settings.debug>
                        <TCEforms>
                            <label>
                                LLL:EXT:location_manager/Resources/Private/Language/locallang.xlf:plugin.locations.settings.advanced.debug
                            </label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </settings.debug>
                </el>

            </ROOT>
        </advanced>

    </sheets>
</T3DataStructure>